      "url": "/puppy-guide.jpg",
      "thumbnailURL": "/puppy-guide.jpg",
      "filename": "puppy-guide.png",
      "mimeType": "image/png",
      "filesize": 312000,
      "width": 800,
      "height": 533,
      "focalX": 50,
      "focalY": 50,
      "sizes": {
        "thumbnail": {
          "url": "/puppy-guide.jpg",
